<%= form_for(@dream) do |f| %>
<%= f.label :title %>
<%= f.text_field :title %><br>
<%= f.label :description %>
<%= f.text_area :description %><br>
<%= f.label :date %>
<%= f.date_field :date %><br><br>
<%= f.label "What type of dream was this?" %>
<%= f.select :dream_type, %w(dream nightmare) %> <br><br>
<%= f.label :elements %><br>

<%= f.collection_check_boxes :element_ids, Element.all, :id, :name do |b| %>
  <%= b.check_box + " " + b.label + "<br>".html_safe %>
<% end %>
<br>


  <%=f.submit %>
<% end %>
